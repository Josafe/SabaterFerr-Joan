package Refactoritzacio;

public class Fechabisiesto {

	private int anio;
	private int mes;
	private int dia;
	
	public void bisiesto() {
	 if((anio% 400==0) || ((anio% 4 == 0) && (anio%100 !=0)))
         diasMes= 29;
     else 
         diasMes= 28; 
     break;
 }
 if(dia > diasMes) 
     return false;
 else 
     return true;

}
